{
  "author": {
    "name": "qmhc"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "scripts/cz-config.js"
    }
  },
  "dependencies": {
    "@popperjs/core": "^2.11.4",
    "vue": "^3.2.31"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@rollup/plugin-eslint": "^8.0.1",
    "@rollup/plugin-replace": "^3.1.0",
    "@types/file-saver": "^2",
    "@types/jest": "^27.4.1",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "@vitejs/plugin-vue": "^2.2.4",
    "@vitejs/plugin-vue-jsx": "^1.3.8",
    "@vue/eslint-config-standard": "^6.1.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/repl": "^1.0.0",
    "@vue/test-utils": "2.0.0-rc.18",
    "chalk": "^4.1.2",
    "commitizen": "^4.2.4",
    "conventional-changelog-cli": "^2.2.2",
    "cz-customizable": "^6.3.0",
    "enquirer": "^2.3.6",
    "eslint": "^8.11.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-vue": "^8.5.0",
    "execa": "^5.1.1",
    "file-saver": "^2.0.5",
    "fs-extra": "^10.0.1",
    "husky": "^7.0.4",
    "is-ci": "^3.0.1",
    "jest": "^27.5.1",
    "jszip": "^3.7.1",
    "lint-staged": "^12.3.7",
    "pinst": "^2.1.6",
    "postcss": "^8.4.12",
    "postcss-discard-duplicates": "^5.1.0",
    "postcss-html": "^1.3.0",
    "postcss-preset-env": "^7.4.3",
    "prettier": "^2.6.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.70.1",
    "sass": "^1.49.9",
    "semver": "^7.3.5",
    "stylelint": "^14.6.0",
    "stylelint-config-html": "^1.0.0",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-prettier": "^2.0.0",
    "ts-jest": "^27.1.4",
    "typescript": "4.5.5",
    "vite": "^2.8.6",
    "vite-plugin-dts": "^1.0.2",
    "vite-plugin-html": "^3.2.0",
    "vue-eslint-parser": "^8.3.0"
  },
  "engines": {
    "node": "^12 || >=14"
  },
  "files": [
    "design",
    "dist",
    "icons",
    "lib",
    "style",
    "themes"
  ],
  "keywords": [
    "flex",
    "components",
    "vue",
    "vexip",
    "plugin",
    "toolkit",
    "vexip-ui",
    "vite"
  ],
  "license": "MIT",
  "main": "dist/vexip-ui.cjs.js",
  "module": "dist/vexip-ui.es.js",
  "name": "vexip-ui",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qmhc/vexip-ui.git"
  },
  "scripts": {
    "build": "node scripts/build.js",
    "build:clean": "yarn run clean && yarn run build",
    "build:icons": "node scripts/build-icons.js",
    "build:lib": "node scripts/build.js --lib",
    "build:playground": "cd playground && vite build",
    "bootstrap": "node scripts/bootstrap.js",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s --commit-path .",
    "clean": "rimraf node_modules/.cache && rimraf node_modules/.vite && rimraf node_modules/.rts2_cache",
    "commit": "git-cz",
    "create": "node scripts/create.js",
    "lint": "eslint --fix --ext .js,.ts,.tsx,.vue common/** components/**",
    "lint:style": "stylelint **/*.{vue,scss} --fix",
    "postinstall": "is-ci || husky install",
    "postpublish": "pinst --enable",
    "precommit": "lint-staged -c ./.husky/.lintstagedrc",
    "prepublishOnly": "pinst --disable",
    "prettier": "prettier --write \"{common,component}/**/*.{ts,js,json,css,pcss,scss,vue,html,md}\"",
    "release": "node scripts/release.js",
    "serve": "node scripts/serve.js",
    "serve:example": "cd example && vite",
    "serve:playground": "cd playground && vite"
  },
  "sideEffects": [
    "dist/*",
    "lib/**",
    "icons/**",
    "*.scss"
  ],
  "typings": "lib/index.d.ts",
  "version": "1.2.1"
}
